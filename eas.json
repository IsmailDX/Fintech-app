{
    "cli": {
        "version": ">= 16.28.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "node": "18.18.0",
            "android": { "buildType": "apk" },
            "env": { "EAS_BUILD_NPM_FLAGS": "--legacy-peer-deps" }
        },
        "preview": {
            "distribution": "internal",
            "node": "18.18.0",
            "android": { "buildType": "apk" },
            "env": { "EAS_BUILD_NPM_FLAGS": "--legacy-peer-deps" }
        },
        "production": {
            "autoIncrement": true,
            "node": "18.18.0",
            "android": { "buildType": "apk" },
            "env": { "EAS_BUILD_NPM_FLAGS": "--legacy-peer-deps" }
        }
    },
    "submit": { "production": {} }
}
